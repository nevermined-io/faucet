{
    "parser": "babel-eslint",
    "extends": [
        "plugin:prettier/recommended",
        "prettier/react",
        "prettier/standard"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error"
    },
    "env": {
        "es6": true,
        "browser": true,
        "mocha": true
    },
    "globals": {
        "expect": true
    }
}
